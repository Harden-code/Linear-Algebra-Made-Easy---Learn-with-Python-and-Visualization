    "from sympy import *\n",
    "from sympy.abc import x,y,z,a,b,c,d\n",
    "# 面积法\n",
    "all=(a+b)*(c+d)\n",
    "rec=2*b*c\n",
    "tri1=1/2*b*d*2\n",
    "tri2=a*c\n",
    "simplify(all-rec-tri1-tri2)\n",
    "\n"
   ]
